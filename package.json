{
  "name": "io-node-challenge",
  "version": "1.0.0",
  "author": "io-developers",
  "license": "ISC",
  "scripts": {
    "dev": "nodemon --watch src --exec \"tsc && node dist/index.js\"",
    "test": "jest",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write 'src/**/*.ts' --config ./.prettierrc",
    "prepare": "husky",
    "start": "node dist/index.js",
    "build": "npx tsc",
    "lambda:package": "npm run build && powershell Compress-Archive -Path dist,node_modules -DestinationPath lambda-deploy.zip"
  },
  "dependencies": {
    "@vendia/serverless-express": "^4.12.6",
    "axios": "^1.7.7",
    "express": "^4.19.2",
    "express-validator": "^7.2.0",
    "reflect-metadata": "^0.2.2",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "8.10.130",
    "@types/jest": "29.5.5",
    "@types/node": "20.14.1",
    "@typescript-eslint/eslint-plugin": "6.9.1",
    "@typescript-eslint/parser": "6.9.1",
    "eslint": "8.8.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "9.1.4",
    "jest": "29.7.0",
    "jest-mock-extended": "3.0.5",
    "nodemon": "^3.1.4",
    "prettier": "3.0.3",
    "ts-jest": "29.1.2",
    "ts-node": "10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "4.7.4"
  }
}
