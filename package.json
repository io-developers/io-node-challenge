{
  "name": "io-node-challenge",
  "version": "1.0.0",
  "author": "io-developers",
  "license": "ISC",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write 'src/**/*.ts' --config ./.prettierrc",
    "prepare": "husky",
    "start": "npm run build && serverless offline start --reloadHandler",
    "deploy": "npm run build && serverless deploy --region us-east-2"
  },
  "dependencies": {
    "@middy/core": "3.4.0",
    "@middy/http-error-handler": "5.4.7",
    "@middy/http-json-body-parser": "3.4.0",
    "axios": "1.7.7",
    "uuid": "10.0.0"
  },
  "devDependencies": {
    "@serverless/typescript": "^3.0.0",
    "@types/aws-lambda": "8.10.130",
    "@types/jest": "29.5.5",
    "@types/node": "20.14.1",
    "@typescript-eslint/eslint-plugin": "6.9.1",
    "@typescript-eslint/parser": "6.9.1",
    "aws-sdk": "2.1542.0",
    "esbuild": "^0.14.11",
    "eslint": "8.8.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "9.1.4",
    "jest": "29.7.0",
    "jest-mock-extended": "3.0.5",
    "prettier": "3.0.3",
    "reflect-metadata": "^0.2.2",
    "serverless": "3.38.0",
    "serverless-esbuild": "1.23.3",
    "serverless-offline": "13.3.3",
    "serverless-plugin-reducer": "^4.0.1",
    "serverless-plugin-typescript": "2.1.5",
    "serverless-step-functions": "3.21.0",
    "ts-jest": "29.1.2",
    "ts-node": "10.9.1",
    "typescript": "4.7.4"
  }
}
